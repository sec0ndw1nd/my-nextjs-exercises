{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["next", "plugin:prettier/recommended"],
  "overrides": [
    {
      "env": {
        "node": true
      },
      "files": [".eslintrc.{js,cjs}"],
      "parserOptions": {
        "sourceType": "script"
      }
    }
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [],
  "rules": {
    "no-console": "off",
    "react/react-in-jsx-scope": "off",
    "no-undef": "error",
    "react/no-unknown-property": "off",
    "import/no-unresolved": "error"
  }
}
